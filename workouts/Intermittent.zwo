
  <workout_file>
    <name> Intermittent</name>
    <description>Intermittent: This training is a way to introduce short high intensity riding alternating it with short recovery, allowing the rider to recover and accumulate time at an intensity that would be difficult to maintain if ridden consecutively.</description>
    <author>Zwift</author>
    <sportType>bike</sportType>
    <tags>
      <tag name="recovery"></tag>
    </tags>
    <workout>
      <Warmup Duration="600" PowerLow="0.4" PowerHigh="0.73" />
<SteadyState Duration="300" Cadence="90" Power="0.88" />
<SteadyState Duration="120" Cadence="85" Power="0.53" />
<IntervalsT Repeat="2" OnDuration="120" OffDuration="30" OnPower="0.88" OffPower="0.88"/>
<SteadyState Duration="120" Cadence="85" Power="0.53" />
<SteadyState Duration="300" Cadence="95" Power="0.88" />
<SteadyState Duration="120" Cadence="85" Power="0.53" />
<IntervalsT Repeat="6" OnDuration="30" OffDuration="30" OnPower="1" OffPower="0.73"/>
<SteadyState Duration="240" Cadence="85" Power="0.53" />
<IntervalsT Repeat="6" OnDuration="30" OffDuration="30" OnPower="1" OffPower="0.73"/>
<SteadyState Duration="240" Cadence="85" Power="0.53" />
<IntervalsT Repeat="6" OnDuration="30" OffDuration="30" OnPower="1" OffPower="0.73"/>
<Cooldown Duration="300" PowerLow="0.53" PowerHigh="0.4" />
    </workout>
  </workout_file>
  