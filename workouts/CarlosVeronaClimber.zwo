<IntervalsT Repeat="1" OnDuration="90" OffDuration="60" OnPower="1.15" OffPower="0.5"/>
<IntervalsT Repeat="1" OnDuration="60" OffDuration="40" OnPower="1.15" OffPower="0.5"/>
<IntervalsT Repeat="4" OnDuration="40" OffDuration="30" OnPower="1.15" OffPower="0.5"/>
