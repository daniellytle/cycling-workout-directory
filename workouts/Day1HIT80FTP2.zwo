<tag name="tempo"></tag>
<tag name="intervals"></tag>
<tag name="recovery"></tag>
<tag name="recovery"></tag>
    </tags>
    <workout>
      <SteadyState Duration="180" Power="0.5" />
<IntervalsT Repeat="4" OnDuration="10" OffDuration="50" OnPower="1.75" OffPower="0.5"/>
<SteadyState Duration="180" Cadence="95" Power="0.7" />
<SteadyState Duration="120" Power="0.5" />
<SteadyState Duration="300" Cadence="90" Power="0.9" />
