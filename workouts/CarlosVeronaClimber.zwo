<SteadyState Duration="60" Power="0.4" />
<IntervalsT Repeat="1" OnDuration="120" OffDuration="90" OnPower="1.15" OffPower="0.5"/>
<IntervalsT Repeat="1" OnDuration="90" OffDuration="60" OnPower="1.15" OffPower="0.5"/>
