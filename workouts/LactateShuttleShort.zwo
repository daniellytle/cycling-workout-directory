<SteadyState Duration="180" Cadence="100" Power="1.2" />
<IntervalsT Repeat="8" OnDuration="60" OffDuration="60" OnPower="0.7" OffPower="1.1"/>
<SteadyState Duration="60" Cadence="100" Power="1.2" />
<SteadyState Duration="120" Cadence="85" Power="0.5" />
    </workout>
