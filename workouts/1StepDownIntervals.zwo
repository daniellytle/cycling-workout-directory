<SteadyState Duration="300" Power="0.75" />
<SteadyState Duration="180" Power="0.85" />
<SteadyState Duration="180" Power="0.9" />
<Cooldown Duration="240" PowerLow="0.9" PowerHigh="0.6" />
<SteadyState Duration="300" Power="0.6" />
<Warmup Duration="300" PowerLow="0.6" PowerHigh="0.95" />
<SteadyState Duration="60" Power="1.25" />
