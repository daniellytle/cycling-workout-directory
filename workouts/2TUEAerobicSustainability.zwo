<IntervalsT Repeat="3" OnDuration="30" OffDuration="210" OnPower="0.9" OffPower="0.74"/>
<SteadyState Duration="180" Cadence="85" Power="0.5" />
<IntervalsT Repeat="3" OnDuration="30" OffDuration="210" OnPower="0.9" OffPower="0.75"/>
<SteadyState Duration="180" Cadence="85" Power="0.5" />
<IntervalsT Repeat="3" OnDuration="30" OffDuration="210" OnPower="0.91" OffPower="0.75"/>
